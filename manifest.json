{
    "name": "FixBlock YTO",
    "version": "1.0.0",
    "manifest_version": 2,
	  "web_accessible_resources": ["script.js"],
	
    "content_scripts": [
      {
        "js": ["contentscript.js"],
        "matches": ["http://www.youtube.com/*&safe=active"]
      }
    ],

  "permissions": [ "activeTab" ]
}